{
  "name": "@clawdbot/discord-user",
  "version": "1.0.0",
  "type": "module",
  "description": "Clawdbot Discord user account (selfbot) channel plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
